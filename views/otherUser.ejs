<%= addFiles %>
<div class='container moveDown'>
  <h1><%= foundUser.name %>'s Profile</h1>
  <div id="profileDiv">
    <div class="col-md-2 pull-md-10">
      <% if(foundUser.image){ %>
        <img src='<%= foundUser.image %>' style='height:160px;width:160px'; />
        <% } %>
      </div>
      <div class="col-md-10 push-md-2">
        <% if(foundUser.contact){ %>
          <ul class = 'list-group'>
          <li class='list-group-item'>Contact:  <%= foundUser.contact %></li>
          </ul>
        <% } %>
        <% if(foundUser.aboutme){ %>
          <ul class='list-group'>
          <li class='list-group-item'>About Me:  <%= foundUser.aboutme %></li>
          </ul>
         <% } %>
      </div>
  </div>
</div>

<table class="table table-hover text-center" id='tableAddFilesOtherUser'>
  <% if(addFiles.length > 0) { %>
  <thead>
      <tr>
          <th class='text-center'>Files</th>
          <th class='text-center'>Title</th>
          <th class='text-center'>Description</th>
          <th></th>
      </tr>
  </thead>
  <tbody>
      <% addFiles.forEach(function (eachAddedFile) {%>
      <tr>
        <td><video controls>
        <a href = "auth/profile/video/<%= eachAddedFile.id %>"></a>
        <source src = "<%= eachAddedFile.addFileProperties %>" type = 'video/mp4'>
        </video>
        </td>
        <td>
        <%= eachAddedFile.title %>
        </td>
        <td>
        <%= eachAddedFile.description %>
        </td>
      </tr>
      <% }) %>
     <% } %>
     </tbody>
</table>
</div
